{
  ".text.tex.latex": {
    "property": [
      {
        "displayText": "!cleanPatterns",
        "snippet": "% !TEX cleanPatterns = $1",
        "description": "Specify patterns to use for latex:clean"
      },
      {
        "displayText": "!enableSynctex",
        "snippet": "% !TEX enableSynctex = $1",
        "description": "Override synctex setting"
      },
      {
        "displayText": "!enableExtendedBuildMode",
        "snippet": "% !TEX enableExtendedBuildMode = $1",
        "description": "Override extended build mode setting"
      },
      {
        "displayText": "!enableShellEscape",
        "snippet": "% !TEX enableShellEscape = $1",
        "description": "Override shell escape setting"
      },
      {
        "displayText": "!engine",
        "snippet": "% !TEX engine = $1",
        "description": "Set engine to compile with"
      },
      {
        "displayText": "!moveResultToSourceDirectory",
        "snippet": "% !TEX moveResultToSourceDirectory = $1",
        "description": "Override move result to source directory setting"
      },
      {
        "displayText": "!outputFormat",
        "snippet": "% !TEX outputFormat = $1",
        "description": "Override the output format"
      },
      {
        "displayText": "!jobNames",
        "snippet": "% !TEX jobNames = $1",
        "description": "Control the number and names of build jobs"
      },
      {
        "displayText": "!outputDirectory",
        "snippet": "% !TEX outputDirectory = $1",
        "description": "Specify the output directory that should be used"
      },
      {
        "displayText": "!producer",
        "snippet": "% !TEX producer = $1",
        "description": "Override the PDF producer"
      },
      {
        "displayText": "!root",
        "snippet": "% !TEX root = $1",
        "description": "Specify the root file that should be built"
      },
      {
        "displayText": "!syntax",
        "snippet": "% !TEX syntax = $1",
        "description": "Change the current syntax highlighting"
      },
      {
        "displayText": "!bib",
        "snippet": "% !TEX bib = $1",
        "description": "Specify bibliography file path"
      }
    ],
    "snippet": [
      {
        "displayText": "\\begin",
        "snippet": "\\\\begin{$1}$2\n\t$3\n\\\\end{$1}",
        "description": "Begin new environment",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Environments"
      },
      {
        "displayText": "\\document",
        "snippet": "\\\\begin{document}\n$1\n\\\\end{document}"
      },
      {
        "displayText": "\\align",
        "snippet": "\\\\begin{align$2}\n\t$1$3\n\\\\end{align$2}",
        "description": "Begin amsmath align environment",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Aligning_equations_with_amsmath#!#Aligning_several_equations"
      },
      {
        "displayText": "\\figure",
        "snippet": "\\\\begin{figure}[$1]\n\t\\\\centering\n\t\\\\includegraphics{$2}\n\t\\\\caption{$3}\n\t\\\\label{$4}\n\\\\end{figure}"
      },
      {
        "displayText": "\\table",
        "snippet": "\\\\begin{table}[$1]\n\t\\\\centering\n\t\\\\begin{tabular}{$2}\n\t\t$3\n\t\\\\end{tabular}\n\t\\\\caption{$4}\n\t\\\\label{$5}\n\\\\end{table}"
      },
      {
        "displayText": "\\itemize",
        "snippet": "\\\\begin{itemize}\n\t\\\\item $1\n\\\\end{itemize}",
        "description": "Bullet point list"
      },
      {
        "displayText": "\\enumerate",
        "snippet": "\\\\begin{enumerate}\n\t\\\\item $1\n\\\\end{enumerate}",
        "description": "Enumerated list"
      },
      {
        "displayText": "\\description",
        "snippet": "\\\\begin{description}\n\t\\\\item [$1] $2\n\\\\end{description}",
        "description": "Descriptive list"
      },
      {
        "displayText": "\\tikzpicture",
        "snippet": "\\\\begin{tikzpicture}\n\t$1\n\\\\end{tikzpicture}"
      }
    ],
    "import": [
      {
        "displayText": "\\include",
        "snippet": "\\\\include{$1}$2"
      },
      {
        "displayText": "\\input",
        "snippet": "\\\\input{$1}$2"
      }
    ],
    "package": [
      {
        "displayText": "\\usepackage",
        "snippet": "\\\\usepackage$2{$1}$3",
        "description": "Load a package",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Creating_a_document_in_LaTeX#!#The_preamble_of_a_document"
      }
    ],
    "method": [
      {
        "displayText": "\\section",
        "snippet": "\\\\section\\{$1\\}$2"
      },
      {
        "displayText": "\\subsection",
        "snippet": "\\\\subsection\\{$1\\}$2"
      },
      {
        "displayText": "\\subsubsection",
        "snippet": "\\\\subsubsection\\{$1\\}$2"
      },
      {
        "displayText": "\\autocite",
        "snippet": "\\\\autocite$2{$1}$3"
      },
      {
        "displayText": "\\parencite",
        "snippet": "\\\\parencite$2{$1}$3"
      },
      {
        "displayText": "\\cite",
        "snippet": "\\\\cite$2{$1}$3"
      },
      {
        "displayText": "\\textcite",
        "snippet": "\\\\textcite$2{$1}$3"
      },
      {
        "displayText": "\\citetitle",
        "snippet": "\\\\citetitle$2{$1}$3"
      },
      {
        "displayText": "\\footnote",
        "snippet": "\\\\footnote{$1}$2"
      },
      {
        "displayText": "\\footnotemark",
        "snippet": "\\\\footnotemark"
      },
      {
        "displayText": "\\footnotetext",
        "snippet": "\\\\footnotetext{$1}$2"
      },
      {
        "displayText": "\\mintinline",
        "snippet": "\\\\mintinline{$1}$2"
      },
      {
        "displayText": "\\clearpage",
        "snippet": "\\\\clearpage"
      },
      {
        "displayText": "\\newpage",
        "snippet": "\\\\newpage"
      },
      {
        "displayText": "\\documentclass",
        "snippet": "\\\\documentclass$2{$1}$3",
        "description": "Set document class",
        "descriptionMoreURL": "https://en.wikibooks.org/wiki/LaTeX/Document_Structure#Document_classes"
      },
      {
        "displayText": "\\tracingmacros",
        "snippet": "\\\\tracingmacros=$1",
        "description": "Write all macro expansions to the log file"
      },
      {
        "displayText": "\\tracingcommands",
        "snippet": "\\\\tracingcommands=$1",
        "description": "Write all commands to the log file"
      },
      {
        "displayText": "\\tracinglostchars",
        "snippet": "\\\\tracinglostchars=$1",
        "description": "Write missing characters to the log file"
      },
      {
        "displayText": "\\tracingonline",
        "snippet": "\\\\tracingonline=$1",
        "description": "Write diagnostic info to terminal as well"
      },
      {
        "displayText": "\\tracingoutput",
        "snippet": "\\\\tracingoutput=$1",
        "description": "Write shipped box content to log file"
      },
      {
        "displayText": "\\tracingpages",
        "snippet": "\\\\tracingpages=$1",
        "description": "Write page cost calculations to log file"
      },
      {
        "displayText": "\\tracingparagraphs",
        "snippet": "\\\\tracingparagraphs=$1",
        "description": "Write line breaking calculations to log file"
      },
      {
        "displayText": "\\tracingrestores",
        "snippet": "\\\\tracingrestores=$1",
        "description": "Write save-stack details to log file"
      },
      {
        "displayText": "\\tracingstats",
        "snippet": "\\\\tracingstats=$1",
        "description": "Write memory usage details to log file"
      },
      {
        "displayText": "\\tracingall",
        "snippet": "\\\\tracingall=$1",
        "description": "Turn on all tracing commands"
      },
      {
        "displayText": "\\tracingassigns",
        "snippet": "\\\\tracingassigns=$1",
        "description": "Trace grouping assignments",
        "rightLabelHTML": "e-teX"
      },
      {
        "displayText": "\\tracinggroups",
        "snippet": "\\\\tracinggroups=$1",
        "description": "Trace group entry and exit",
        "rightLabelHTML": "e-teX"
      },
      {
        "displayText": "\\tracingifs",
        "snippet": "\\\\tracingifs=$1",
        "description": "Trace conditional statements",
        "rightLabelHTML": "e-teX"
      },
      {
        "displayText": "\\tracingscantokens",
        "snippet": "\\\\tracingscantokens=$1",
        "description": "Trace \\scantokens invocation",
        "rightLabelHTML": "e-teX"
      },
      {
        "displayText": "\\tracingcolors",
        "snippet": "\\\\tracingcolors=$1",
        "description": "Trace color information",
        "rightLabelHTML": "xcolor"
      }
    ]
  },

  ".text.tex.latex .string.other.math": {
    "snippet": [
      {
        "displayText": "\\frac",
