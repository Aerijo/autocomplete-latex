{
  ".text.tex.latex": {
    "property": [
      {
        "displayText": "!cleanPatterns",
        "snippet": "% !TEX cleanPatterns = $1",
        "description": "Specify patterns to use for latex:clean"
      },
      {
        "displayText": "!enableSynctex",
        "snippet": "% !TEX enableSynctex = $1",
        "description": "Override synctex setting"
      },
      {
        "displayText": "!enableExtendedBuildMode",
        "snippet": "% !TEX enableExtendedBuildMode = $1",
        "description": "Override extended build mode setting"
      },
      {
        "displayText": "!enableShellEscape",
        "snippet": "% !TEX enableShellEscape = $1",
        "description": "Override shell escape setting"
      },
      {
        "displayText": "!engine",
        "snippet": "% !TEX engine = $1",
        "description": "Set engine to compile with"
      },
      {
        "displayText": "!moveResultToSourceDirectory",
        "snippet": "% !TEX moveResultToSourceDirectory = $1",
        "description": "Override move result to source directory setting"
      },
      {
        "displayText": "!outputFormat",
        "snippet": "% !TEX outputFormat = $1",
        "description": "Override the output format"
      },
      {
        "displayText": "!jobNames",
        "snippet": "% !TEX jobNames = $1",
        "description": "Control the number and names of build jobs"
      },
      {
        "displayText": "!outputDirectory",
        "snippet": "% !TEX outputDirectory = $1",
        "description": "Specify the output directory that should be used"
      },
      {
        "displayText": "!producer",
        "snippet": "% !TEX producer = $1",
        "description": "Override the PDF producer"
      },
      {
        "displayText": "!root",
        "snippet": "% !TEX root = $1",
        "description": "Specify the root file that should be built"
      }
    ],
    "snippet": [
      {
        "displayText": "\\begin",
        "snippet": "\\\\begin{${1:env}}$2\n\t$3\n\\\\end{${1:env}}",
        "description": "Begin new environment",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Environments"
      },
      {
        "displayText": "\\documentclass",
        "snippet": "\\\\documentclass${2}{${1:class}}\n$3",
        "description": "Set document class",
        "descriptionMoreURL": "https://en.wikibooks.org/wiki/LaTeX/Document_Structure#Document_classes"
      },
      {
        "displayText": "\\align",
        "snippet": "\\%\n\\\\begin{align$2}\n\t$1$3\n\\\\end{align$2}",
        "description": "Begin amsmath align environment",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Aligning_equations_with_amsmath#!#Aligning_several_equations"
      },
      {
        "displayText": "\\section",
        "snippet": "\\\\section\\{$1\\}$2"
      },
      {
        "displayText": "\\subsection",
        "snippet": "\\\\subsection\\{$1\\}$2"
      },
      {
        "displayText": "\\subsubsection",
        "snippet": "\\\\subsubsection\\{$1\\}$2"
      },
      {
        "displayText": "\\figure",
        "snippet": "\\\\begin{figure}[$1]\n\t\\\\centering\n\t\\\\includegraphics{$2}\n\t\\\\caption{$3}\n\t\\\\label{$4}\n\\\\end{figure}\n$5"
      },
      {
        "displayText": "\\autocite",
        "snippet": "\\\\autocite{$1}$2"
      },
      {
        "displayText": "\\footnote",
        "snippet": "\\\\footnote{$1}$2"
      },
      {
        "displayText": "\\footnotemark",
        "snippet": "\\\\footnotemark"
      },
      {
        "displayText": "\\footnotetext",
        "snippet": "\\\\footnotetext{$1}$2"
      },
      {
        "displayText": "\\mintinline",
        "snippet": "\\\\mintinline{$1}$2"
      },
      {
        "displayText": "\\clearpage",
        "snippet": "\\\\clearpage"
      },
      {
        "displayText": "\\newpage",
        "snippet": "\\\\newpage"
      }
    ],
    "import": [
      {
        "displayText": "\\include",
        "snippet": "\\\\include{$1}$2"
      }
    ],
    "package": [
      {
        "displayText": "\\usepackage",
        "snippet": "\\\\usepackage$2{${1:<package>}}\n$3",
        "description": "Load a package",
        "descriptionMoreURL": "https://www.sharelatex.com/learn/Creating_a_document_in_LaTeX#!#The_preamble_of_a_document"
      }
    ]
  },

  ".text.tex.latex .string.other.math": {
    "snippet": [
      {
        "displayText": "\\frac",
        "snippet": "\\\\frac{$1}{$2}$3",
        "description": "Display math fraction",
        "descriptionMoreURL": "https://en.wikibooks.org/wiki/LaTeX/Mathematics"
      },
      {
        "displayText": "\\text",
        "snippet": "\\\\text{$1}$2",
        "description": "Text in mathmode",
        "descriptionMoreURL": "https://en.wikibooks.org/wiki/LaTeX/Mathematics#Adding_text_to_equations"
      }
    ]
  }
}
